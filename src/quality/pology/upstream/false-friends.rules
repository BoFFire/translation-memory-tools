# False friends rules for Catalan translations
# by KDE Catalan Team (kde-i18n-ca@kde.org)
# License: GPLv3

# False friends list adapted from (and others): 
# http://ca.wikipedia.org/wiki/Llista_de_falsos_amics_del_català_amb_l'anglès
# Guia d'estil (any 2010) de Softcatalà

# Remove XML entities.
include file="filter.include"

# Remove accelerators and  hyphen inside words.
addFilterRegex match="(_|~|&)" on="pmsgid,pmsgstr"

# «Absolutelly» es tradueix per «Completament».
{\babsolutelly\b}i
id="ff_SC-absolutelly"
valid msgstr="completament"
valid msgstr="totalment"
valid !msgstr="absolutament"
hint="«Absolutelly» es tradueix per «Completament», no per «Absolutament» (fals amic)"

# «Abstract» es tradueix per «Resum».
{\babstracts?\b}i
id="ff_SC-abstract"
valid msgstr="resums?"
valid after="(element|key|properties|spiral|type).*\s"
valid before="\s(artwork|class|declarator|function|image|key|method|name|pattern|separator|type)"
valid before="\s(green|red|yellow)"
valid before="\s.*(case|method|operation|pattern|socket)"
valid !msgstr="abstract(e|a|es)"
hint="«Abstract» es tradueix per «Resum», no per «Abstracte» (fals amic)"

