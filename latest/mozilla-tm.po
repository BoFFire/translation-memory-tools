msgstr "%S &lt;ordre&gt;: Envia una ordre al MemoServ."

# Translation source: /chat/irc.properties.po from project 'mozilla'
#: command.msg
msgid ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a user (as "
