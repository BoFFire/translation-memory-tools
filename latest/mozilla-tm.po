msgstr "%S [&lt;tema nou&gt;]: Mostra o canvia el tema del canal."

# Translation source: /chat/irc.properties.po from project 'mozilla'
#: command.umode
msgid "%S (+|-)&lt;new mode&gt;: Set or unset a user mode."
msgstr "%S (+|-)&lt;mode nou&gt;: Configura o desconfigura un mode d'usuari."

# Translation source: /chat/irc.properties.po from project 'mozilla'
#: command.version
msgid "%S &lt;nick&gt;: Request the version of a user's client."
msgstr "%S &lt;sobrenom&gt;: Sol·licita la versió del client d'un usuari."

# Translation source: /chat/irc.properties.po from project 'mozilla'
#: command.voice
msgid ""
"%S &lt;nick1&gt;[,&lt;nick2&gt;]*: Grant channel voice status to someone. "
