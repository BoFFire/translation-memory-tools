#: command.quote
msgid "%S &lt;command&gt;: Send a raw command to the server."
msgstr "%S &lt;ordre&gt;: Envia una ordre en brut al servidor."

# Translation source: /chat/irc.properties.po from project 'mozilla'
#: command.time
msgid "%S: Displays the current local time at the IRC server."
msgstr "%S: Mostra l'hora local actual al servidor IRC."

# Translation source: /chat/irc.properties.po from project 'mozilla'
