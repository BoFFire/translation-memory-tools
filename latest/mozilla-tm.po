msgstr "S'ha trobat l'etiqueta inicial “%1$S” dins del cos de la taula."

# Translation source: /dom/chrome/layout/htmlparser.properties.po from project
# 'mozilla'
#: errEndTagSeenWithoutDoctype
msgid ""
"End tag seen without seeing a doctype first. Expected “<!DOCTYPE html>”."
msgstr ""
"S'ha trobat una etiqueta final abans del doctype. S'esperava “<!DOCTYPE "
"html>”."

# Translation source: /dom/chrome/layout/htmlparser.properties.po from project
# 'mozilla'
#: errEndTagAfterBody
