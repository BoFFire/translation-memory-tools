msgctxt "UIMachineSettingsUSB#26"
msgid "<nobr>Serial No.: %1</nobr>"
msgstr "<nobr>No. de sèrie: %1</nobr>"

# Translation source: /virtualbox-ca.po from project 'virtualbox'
#: UIMachineSettingsUSB#27
msgctxt "UIMachineSettingsUSB#27"
msgid "<nobr>Port: %1</nobr>"
msgstr "<nobr>Port: %1</nobr>"

# Translation source: /virtualbox-ca.po from project 'virtualbox'
