#: UIMachineSettingsUSB#23
msgctxt "UIMachineSettingsUSB#23"
msgid "<nobr>Revision: %3</nobr>"
msgstr "<nobr>Revisió: %3</nobr>"

# Translation source: /virtualbox-ca.po from project 'virtualbox'
#: UIMachineSettingsUSB#24
msgctxt "UIMachineSettingsUSB#24"
msgid "<nobr>Product: %4</nobr>"
msgstr "<nobr>Producte: %4</nobr>"

