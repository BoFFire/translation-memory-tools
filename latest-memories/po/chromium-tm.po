# Translation source: /translations/remoting_strings/ca.po from project
# 'chromium'
#. UNUSED: (not pp_ifdef('_google_chrome')) == False
#. note: 'Error message displayed if the user tries to connect to a computer
#. running out-of-date Me2Me host software.'
